{
  "expo": {
    "name": "Flot",
    "slug": "flot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#2E7D32",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.saveursetvie.flot"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2E7D32"
      },
      "package": "com.saveursetvie.flot",
      "permissions": [
        "CAMERA",
        "ACCESS_NETWORK_STATE",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/favicon.png",
      "name": "Flot - Gestion de flotte",
      "shortName": "Flot",
      "description": "Application de gestion de flotte pour conducteurs",
      "backgroundColor": "#2E7D32",
      "themeColor": "#2E7D32"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "L'application Flot a besoin d'acceder a votre camera pour photographier les tickets et vehicules."
        }
      ],
      "expo-notifications",
      "expo-sqlite",
      "expo-asset",
      "expo-font"
    ],
    "scheme": "flot",
    "extra": {
      "eas": {
        "projectId": "3ac2f542-8fa3-4385-a746-b84c0766f6cf"
      }
    },
    "owner": "mondeactu"
  }
}
