{
  "expo": {
    "name": "Flot",
    "slug": "flot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#2E7D32",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.saveursetvie.flot"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#2E7D32"
      },
      "package": "com.saveursetvie.flot"
    },
    "plugins": [
      "expo-router",
      "expo-camera",
      "expo-image-manipulator",
      "expo-notifications",
      "expo-sqlite"
    ],
    "scheme": "flot",
    "extra": {
      "eas": {
        "projectId": "3ac2f542-8fa3-4385-a746-b84c0766f6cf"
      }
    },
    "owner": "mondeactu"
  }
}
